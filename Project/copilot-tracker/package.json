{
  "name": "copilot-tracker",
  "displayName": "Copilot Tracker",
  "description": "Tracks Copilot usage time",
  "version": "1.0.0",
  "publisher": "Jaydip_Changani",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showCopilotUsage",
        "title": "Show Copilot Usage Time"
      }
    ]
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run compile"
  },
  "devDependencies": {
    "typescript": "^4.8.4",
    "vsce": "^2.8.1"
  }
}
